{
    "dashboard": {
      "id": null,
      "uid": "etl_pipeline_dashboard",
      "title": "ETL Pipeline Monitoring",
      "tags": [],
      "timezone": "browser",
      "schemaVersion": 27,
      "version": 1,
      "panels": [
        {
          "type": "graph",
          "title": "Successful ETL Loads",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "successful_loads",
              "interval": "",
              "legendFormat": "Successful Loads",
              "refId": "A"
            }
          ],
          "xaxis": {
            "mode": "time",
            "name": "Time",
            "show": true
          },
          "yaxes": [
            {
              "format": "short",
              "label": "Count",
              "show": true
            },
            {
              "format": "short",
              "label": "Count",
              "show": true
            }
          ],
          "legend": {
            "show": true
          }
        },
        {
          "type": "graph",
          "title": "Failed ETL Loads",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "failed_loads",
              "interval": "",
              "legendFormat": "Failed Loads",
              "refId": "A"
            }
          ],
          "xaxis": {
            "mode": "time",
            "name": "Time",
            "show": true
          },
          "yaxes": [
            {
              "format": "short",
              "label": "Count",
              "show": true
            },
            {
              "format": "short",
              "label": "Count",
              "show": true
            }
          ],
          "legend": {
            "show": true
          }
        },
        {
          "type": "table",
          "title": "Recent ETL Log Messages",
          "datasource": "ElasticSearch",
          "targets": [
            {
              "index": "etl-logs-*",
              "query": "*",
              "refId": "A",
              "size": 10,
              "sort": {
                "order": "desc",
                "field": "@timestamp"
              }
            }
          ],
          "columns": [
            {
              "text": "Timestamp",
              "type": "time",
              "field": "@timestamp"
            },
            {
              "text": "Log Level",
              "field": "level"
            },
            {
              "text": "Message",
              "field": "message"
            }
          ],
          "options": {
            "showHeader": true
          }
        }
      ]
    },
    "overwrite": false
  }